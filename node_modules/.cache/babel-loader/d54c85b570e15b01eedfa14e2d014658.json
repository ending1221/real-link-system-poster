RL(text, opts, (err, url) => {\n\t\t\tif (err) throw err\n\t\t\t// console.log('url: ',url);\n\t\t\tqrcodeUrl.value = url\n\t\t});\n\t};\n\t\n\treturn {\n\t\tname,\n\t\tid,\n\t\topenPoster,\n\t\tshowPoster,\n\t\tqrcodeUrl\n\t}\n}\n}\n</script>\n\n<style>\nbody, html {\n\tpadding: 0;\n\tmargin: 0;\n}\n#app {\n\tfont-family: Avenir, Helvetica, Arial, sans-serif;\n\t-webkit-font-smoothing: antialiased;\n\t-moz-osx-font-smoothing: grayscale;\n}\n.poster-page {\n\twidth: 930px;\n    padding: 0px;\n    margin-left: auto;\n    margin-right: auto;\n    z-index: 1;\n}\n</style>\n"],"sourceRoot":""}]}